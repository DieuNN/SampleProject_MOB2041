<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="8dp"
    android:id="@+id/librarian_item">


    <TextView
        android:textStyle="bold"
        android:textSize="18sp"
        android:id="@+id/txt_display_name_librarian_item"
        android:text="Display name example"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <TextView
        android:layout_marginTop="8dp"
        android:layout_below="@+id/txt_display_name_librarian_item"
        android:id="@+id/txt_login_name_librarian_item"
        android:text="Login name example"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>



</RelativeLayout>